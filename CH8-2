#include<stdio.h>
#include<conio.h>
main()
{
	int i=1;
	clrscr();

	do
	{
		printf("%d ",i);
		i++;
	}while(i <= 10);

	getch();
}


    SECOND PROGRAM


#include<stdio.h>
#include<conio.h>
main()
{
	int i=1,n;
	clrscr();

	printf("Enter n: ");
	scanf("%d",&n);

	do
	{
		if(i%2==0)
		{
			printf("%d ",i);
		}
		i++;
	}while(i <= n);


	getch();
}
